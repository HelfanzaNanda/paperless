<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ydhnwb.paperlessapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApp"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:replace="android:allowBackup">
        <activity
            android:name=".ui.all_my_store.AllMyStoreActivity"
            android:label="@string/title_activity_all_my_store"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".ui.update_profile.UpdateProfileActivity"
            android:label="@string/title_activity_update_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.quickupdate.QuickUpdateActivity"
            android:label="@string/title_activity_quick_update"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.user_history.UserHistoryActivity"
            android:label="@string/title_activity_user_history"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.detail_order.DetailOrderActivity"
            android:label="@string/title_activity_detail_order"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.analytic.AnalyticActivity"
            android:label="@string/title_activity_analytic"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.store_page.StorePageActivity"
            android:label="@string/title_activity_store_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.detail_product.DetailProductActivity"
            android:label="@string/title_activity_detail_product"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.catalog.CatalogActivity"
            android:label="@string/title_activity_catalog"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.scanner.ShowQRActivity" />
        <activity
            android:name=".ui.invitation.InvitationActivity"
            android:label="@string/title_activity_invitation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.search_user.SearchUserActivity"
            android:label="@string/title_activity_search_user"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.scanner.ScannerActivity" />
        <activity
            android:name=".ui.checkout.CheckoutActivity"
            android:label="@string/title_activity_checkout"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.product.ProductActivity"
            android:label="@string/title_activity_product"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.store.CreateStoreActivity"
            android:label="@string/title_activity_create_store"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.reset_password.ResetPasswordActivity"
            android:label="@string/title_activity_reset_password"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.manage.ManageActivity"
            android:label="@string/title_activity_manage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.register.RegisterActivity" />
        <activity android:name=".ui.IntroActivity" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity
            android:name=".ui.main.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".utilities.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>